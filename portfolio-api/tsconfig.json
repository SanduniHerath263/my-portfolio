{
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",

    "rootDir": "src",
    "outDir": "dist",

    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,

    "sourceMap": true,
    "resolveJsonModule": true,

    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true




  }
}
